<!DOCTYPE html>
<html lang="en">
<head>
	<% include ../partials/head %>
</head>
<body class="container">
	<header>
		<% include ../partials/header %>
	</header>
	<main>
		<div class="jumbotron">
			<p><i class="mdi-action-favorite mdi-material-red" style="font-size:34px"></i> YAAAAY, this site was build with node.js</p>
		
			<h2>Variable</h2>
			<p><%= tagline %></p>
			<h2>Loop</h2>
			<ul>
				<% tools.forEach(function(tool) { %>
				<li><%= tool.name %></li>
				<% }); %>
			</ul>
		</div>
	</main>
	<footer>
        <% include ../partials/footer %>
    </footer>
</body>
</html>